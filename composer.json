{
  "name": "carno-php/env",
  "license": "MIT",
  "require": {
    "php": "^7.1",
    "symfony/dotenv": "~4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7"
  },
  "autoload": {
    "psr-4": {
      "Carno\\Env\\": "src"
    },
    "files": ["src/utils.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "Carno\\Env\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
